{
    "name": "khristophor/wordpress-stats",
    "description": "Displays statistics about the WordPress install on the front end.",
    "type": "wordpress-plugin",
    "keywords": ["wordpress", "plugin"],
    "homepage": "https://github.com/khristophor/wordpress-stats/",
    "license": "GPL-2.0+",
    "authors": [
      {
          "name": "Chris Wiseman",
          "email": "khristophor@github.io",
          "homepage": "https://github.com/khristophor/wordpress-stats/"
      }
    ],
    "support": {
        "issues": "https://github.com/khristophor/wordpress-stats/",
        "source": "https://github.com/khristophor/wordpress-stats/"
    },
    "autoload": {
        "psr-4": {
            "WordpressStats\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\WordpressStats\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.0.0",
        "composer/installers": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "5.7.0",
        "squizlabs/php_codesniffer": "2.8.1",
        "wp-coding-standards/wpcs": "0.*",
        "10up/wp_mock": "dev-dev"
    },
    "suggest": {}
}
